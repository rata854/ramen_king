<div class='container'>
  <div class='row'>
    <div class="col-sm-12 col-md-8 col-lg-8 mx-auto">
    <h4>新規口コミ</h4>
      <%= form_with(model:[@store,@store_comment], url: store_store_comments_path, local:true) do |f| %>
      <%= render 'layouts/errors', model: @store_comment %>
      <%= render 'form', f: f %>
        <div class="form-group">
          <%= f.submit class: 'btn btn-secondary' %>
        </div>
      <% end %>
  </div>
</div>