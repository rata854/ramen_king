<!DOCTYPE html>
<html>
  <head>
    <title>RamenKing</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div id="wrapper">
      <main>
        <p id="notice"><%= notice %></p>
      </main>
      <%= yield %>
    </div>
    <%= render 'layouts/footer' %>
  </body>
</html>